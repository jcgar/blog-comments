{
  "name": "bloggur",
  "version": "0.0.1",
  "description": "Simple blog app demonstrating server rendering and routes using `react-redux-provide`.",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist coverage",
    "prepublish": "npm run clean && npm run build",
    "test": "npm run prepublish && mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "start": "npm run build && npm run server",
    "start:dev": "npm run server:dev",
    "server": "NODE_ENV=production MIN_EXT=.min node server.production.js",
    "server:dev": "NODE_ENV=development node server.development.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loggur/bloggur.git"
  },
  "author": "timbur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loggur/bloggur/issues"
  },
  "homepage": "https://github.com/loggur/bloggur#readme",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "provide-map": "^0.5.0",
    "provide-page": "^0.2.0",
    "provide-theme": "^0.3.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux-provide": "^5.2.0",
    "react-remarkable": "^1.1.1",
    "redux-replicate": "^0.3.0",
    "redux-replicate-fs": "^0.3.0",
    "redux-replicate-localforage": "^0.3.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "chokidar": "^1.4.1",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "isparta": "^3.0.3",
    "istanbul": "^0.3.17",
    "jsdom": "~5.4.3",
    "mocha": "^2.2.5",
    "postcss": "^5.0.14",
    "postcss-loader": "^0.8.0",
    "postcss-modules": "^0.1.3",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "react-redux-provide-test-utils": "^0.1.3",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.3.4",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
